{% load templatetag_handlebars %}
{% load i18n %}
{% load static %}

{% tplhandlebars "page" %}
    <div class="container">
        {{#if view.showTitle}}
            <div class="title">
                <h1>{{title}}</h1>
            </div>
        {{/if}}
        {{{body}}}
    </div>

{% endtplhandlebars %}


{% tplhandlebars "error/not_found" %}

    <section class="wrapper">
        <article class="wallpost">
            <header>
                <h2>Something went wrong here...</h2>
            </header>
        </article>

        <div class="wall">
            The page or object you were looking for couldn't be found.
        </div>
    </section>

{% endtplhandlebars %}